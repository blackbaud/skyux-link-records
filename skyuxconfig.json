{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-link-records",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "moment"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  }
}
